<?xml version="1.0" encoding="Windows-1252"?>
<Model version="3">
    <Title>MPB Cold Tolerance (Annual)</Title>
    <Version>1.0</Version>
    <ID>.CTs</ID>
    <EXE>MPB-ColdTolerance.dll</EXE>
    <Behaviour>0</Behaviour>
    <Description>Modelling cold tolerance in the mountain pine beetle.</Description>
    <WindowRect>0 0 167 461</WindowRect>
    <SimulatedCategory>TN T TX</SimulatedCategory>
    <TransferFileVersion>2</TransferFileVersion>
    <SSI/>
    <NbYearMin>2</NbYearMin>
    <NbYearMax>999</NbYearMax>
    <ThreadSafe>true</ThreadSafe>
    <InputVariableArray>
        <InputVariable>
            <Name>GenerateMicroclimate</Name>
            <Caption>Generate microclimate:</Caption>
            <Description>True if weather input is air temperature and false in weather input is phloem temperature</Description>
            <Type>0</Type>
            <Rect>90 10 110 200</Rect>
            <SeparatorPos>125</SeparatorPos>
            <DefaultValue>1</DefaultValue>
            <MinValue>0</MinValue>
            <MaxValue>1</MaxValue>
            <TextValue/>
        </InputVariable>
        <InputVariable>
            <Name>Header1</Name>
            <Caption>Moutain Pine Beetle Cold Tolerance Model</Caption>
            <Description/>
            <Type>8</Type>
            <Rect>5 10 25 275</Rect>
            <SeparatorPos>100</SeparatorPos>
            <DefaultValue>0</DefaultValue>
            <MinValue>-999</MinValue>
            <MaxValue>999</MaxValue>
            <TextValue/>
        </InputVariable>
        <InputVariable>
            <Name>Header2</Name>
            <Caption>Régnière, J. and B. Bentz. 2007. Modelling cold tolerance</Caption>
            <Description/>
            <Type>8</Type>
            <Rect>40 10 60 405</Rect>
            <SeparatorPos>100</SeparatorPos>
            <DefaultValue>0</DefaultValue>
            <MinValue>-999</MinValue>
            <MaxValue>999</MaxValue>
            <TextValue/>
        </InputVariable>
        <InputVariable>
            <Name>Header3</Name>
            <Caption>in the mountain pine beetle, Dendroctonus ponderosae.</Caption>
            <Description/>
            <Type>8</Type>
            <Rect>60 30 80 405</Rect>
            <SeparatorPos>100</SeparatorPos>
            <DefaultValue>0</DefaultValue>
            <MinValue>-999</MinValue>
            <MaxValue>999</MaxValue>
            <TextValue/>
        </InputVariable>
    </InputVariableArray>
    <OutputTypeMode>0 0</OutputTypeMode>
    <MissingValue>-999</MissingValue>
    <OutputVariableArray>
        <OutputVariable>
            <Name>Tmin</Name>
            <Title>Tmin</Title>
            <Units>°C</Units>
            <Description>Minimum daily phloem temperatures reach in the winter beginning the first of August of the preceding year.</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>Psurv</Name>
            <Title>Psurv</Title>
            <Units>%</Units>
            <Description>Minimum seasonal survival</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>3</Precision>
        </OutputVariable>
    </OutputVariableArray>
