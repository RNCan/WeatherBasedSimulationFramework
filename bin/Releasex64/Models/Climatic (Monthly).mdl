<?xml version="1.0" encoding="Windows-1252"?>
<Model version="3">
    <Title>Climatic Monthly</Title>
    <Version>2.0</Version>
    <ID>.null</ID>
    <EXE>Climatic.dll</EXE>
    <Behaviour>0</Behaviour>
    <Description>This model compute climatic variable by month</Description>
    <WindowRect>0 0 83 386</WindowRect>
    <SimulatedCategory>TN T TX P TD H WS R</SimulatedCategory>
    <TransferFileVersion>2</TransferFileVersion>
    <SSI/>
    <NbYearMin>1</NbYearMin>
    <NbYearMax>999</NbYearMax>
    <ThreadSafe>true</ThreadSafe>
    <InputVariableArray>
        <InputVariable>
            <Name>Head</Name>
            <Caption>No input</Caption>
            <Description/>
            <Type>10</Type>
            <Rect>10 10 32 160</Rect>
            <SeparatorPos>100</SeparatorPos>
            <DefaultValue>0</DefaultValue>
            <MinValue>-999</MinValue>
            <MaxValue>999</MaxValue>
            <TextValue/>
        </InputVariable>
    </InputVariableArray>
    <OutputTypeMode>1 0</OutputTypeMode>
    <MissingValue>-9999</MissingValue>
    <OutputVariableArray>
        <OutputVariable>
            <Name>LowestTMin</Name>
            <Title>Lowest TMin</Title>
            <Units>°C</Units>
            <Description>Lowest daily minimum temperature of the month</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>MeanTMin</Name>
            <Title>Mean TMin</Title>
            <Units>°C</Units>
            <Description>Monthly mean of daily minimum temperature</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>MeanTMean</Name>
            <Title>Mean TMean</Title>
            <Units>°C</Units>
            <Description>Monthly mean of daily mean temperature</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>MeanTMax</Name>
            <Title>Mean TMax</Title>
            <Units>°C</Units>
            <Description>Monthly mean of daily maximum temperature</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>HighestTMax</Name>
            <Title>Highest TMax</Title>
            <Units>°C</Units>
            <Description>Highest daily maximum temperature of the month</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>TotalPrecipitation</Name>
            <Title>Total Precipitation</Title>
            <Units>mm</Units>
            <Description>Monthly total precipitation</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>TDew</Name>
            <Title>DewPoint</Title>
            <Units>°C</Units>
            <Description>Monthly mean of daily mean dew point temperature</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>RelativeHumidity</Name>
            <Title>RelativeHumidity</Title>
            <Units>%</Units>
            <Description>Monthly mean of daily mean relative humidity</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>WindSpeed</Name>
            <Title>WindSpeed</Title>
            <Units>km/h</Units>
            <Description>Mean wind speed at 10 meters (km/h)</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>FrostDay</Name>
            <Title>Frost Days</Title>
            <Units>days</Units>
            <Description>Number of days in the year when the daily minimum temperature is &lt; 0 °C</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>FrostFreeDay</Name>
            <Title>FrostFreeDay</Title>
            <Units>days</Units>
            <Description>Number of days in the year when the daily minimum temperature is &gt;= 0 °C</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>WetDay</Name>
            <Title>WetDay</Title>
            <Units>days</Units>
            <Description>Number of days in the year when the daily precipitation is &gt;= 0.2 mm</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>DryDay</Name>
            <Title>DryDay</Title>
            <Units>days</Units>
            <Description>Number of days in the year when the daily precipitation is &lt; 0.2 mm</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
        <OutputVariable>
            <Name>TotalRadiation</Name>
            <Title>TotalRadiation</Title>
            <Units>MJ/m²</Units>
            <Description>Monthly total radiation computes by MTCLIM. See documentation of MTCLIM 4.3</Description>
            <TimeMode>4 0</TimeMode>
            <Precision>1</Precision>
        </OutputVariable>
    </OutputVariableArray>
